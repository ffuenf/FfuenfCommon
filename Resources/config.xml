<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/shopware/shopware/5.5/engine/Shopware/Components/Plugin/schema/config.xsd">
    <elements>
        <element type="select">
            <name>debug</name>
            <label lang="de">debug logging aktiviert</label>
            <label lang="en">debug logging enabled</label>
            <value>0</value>
            <store>
                <option>
                    <value>1</value>
                    <label lang="de">Ja</label>
                    <label lang="en">Yes</label>
                </option>
                <option>
                    <value>0</value>
                    <label lang="de">Nein</label>
                    <label lang="en">No</label>
                </option>
            </store>
        </element>
        <element type="select">
            <name>api_log</name>
            <label lang="de">API logging aktiviert</label>
            <label lang="en">API logging enabled</label>
            <value>0</value>
            <store>
                <option>
                    <value>1</value>
                    <label lang="de">Ja</label>
                    <label lang="en">Yes</label>
                </option>
                <option>
                    <value>0</value>
                    <label lang="de">Nein</label>
                    <label lang="en">No</label>
                </option>
            </store>
        </element>
        <element type="select">
            <name>api_log_verbose</name>
            <label lang="de">Erweitertes API logging aktiviert</label>
            <label lang="en">verbose API logging enabled</label>
            <value>0</value>
            <store>
                <option>
                    <value>1</value>
                    <label lang="de">Ja</label>
                    <label lang="en">Yes</label>
                </option>
                <option>
                    <value>0</value>
                    <label lang="de">Nein</label>
                    <label lang="en">No</label>
                </option>
            </store>
        </element>
        <element type="select">
            <name>backend_design_enabled</name>
            <label lang="de">Angepasstes Backend-Design aktiviert</label>
            <label lang="en">customized backend-design enabled</label>
            <value>1</value>
            <store>
                <option>
                    <value>1</value>
                    <label lang="de">Ja</label>
                    <label lang="en">Yes</label>
                </option>
                <option>
                    <value>0</value>
                    <label lang="de">Nein</label>
                    <label lang="en">No</label>
                </option>
            </store>
        </element>
        <element type="text">
            <name>base64logo</name>
            <label lang="de">base64 enkodiertes Logo</label>
            <label lang="en">base64 encoded logo</label>
            <value></value>
        </element>
        <element type="text">
            <name>environment_variable</name>
            <label lang="de">Umgebungsvariable für Umgebung</label>
            <label lang="en">Environmentvariable for environment</label>
            <value>SHOPWARE_ENV</value>
        </element>
        <element type="textarea">
            <name>whitelistIP</name>
            <label>Whitelist for CSP-Reporting</label>
            <label lang="de">IP-Adressen Whitelist</label>
            <options>
                <helpText>Multiple IPs with a line break</helpText>
            </options>
        </element>
        <element required="true" type="text">
            <name>monitoredCronsMail</name>
            <label lang="de">E-Mail Adresse</label>
            <label lang="en">Email address</label>
            <description lang="de">Hinterlegen Sie Mailadressen an die im Fehlerfall eine Warnmail verschickt wird. Mehrere Mailadressen können Semikolon (;) separiert hinterlegt werden.</description>
        </element>
        <element required="true" type="combo">
            <name>monitoredCronsList</name>
            <label lang="de">Überwachte CronJobs</label>
            <label lang="en">Monitored CronJobs</label>
            <description lang="de">Wählen Sie alle CronJobs aus die überwacht werden sollen.</description>
            <options>
                <multiSelect>true</multiSelect>
                <valueField>id</valueField>
                <displayField>id</displayField>
                <displayTpl><![CDATA[<tpl for=".">{name} ({action})<tpl if="xindex < xcount">, </tpl></tpl>]]></displayTpl>
                <tpl><![CDATA[<tpl for="."><div class="x-boundlist-item"><div><b>Name:</b> {name}</div><div><b>Aktion:</b> {action}</div><b>Aktiv:</b> {active}</div></div></tpl>]]></tpl>
                <store>
                    new Ext.data.Store({
                        fields: [
                            { name: 'id', type: 'int'},
                            { name: 'action', type: 'string'},
                            { name: 'active', type: 'boolean'},
                            { name: 'name', type: 'string'}
                        ],
                        proxy: {
                            type: 'ajax',
                            url: 'Config/getTableList?_repositoryClass=cronJob',
                            reader: {
                                type: 'json',
                                root: 'data'
                            },
                            extraParams: {
                                limit: 1000
                            }
                        },
                        autoLoad: true
                    })
                </store>
            </options>
        </element>
        <element required="true" type="number">
            <name>monitoredCronsDowntime</name>
            <label lang="de">Maximale Ausfallzeit</label>
            <label lang="en">Maximum downtime</label>
            <value>10</value>
            <description lang="de">Maximale Ausfallzeit (in Minuten). Eine Warnung erfolgt erst, wenn die letzten Ausführung addiert mit dem angegebenen Wert in der Vergangenheit liegt und der CronJob weiterhin deaktiviert ist.</description>
        </element>
        <element required="true" type="boolean">
            <name>monitoredCronsOnDispatch</name>
            <label lang="de">Prüfung bei Seitenaufruf?</label>
            <label lang="en">Check on page load?</label>
            <value>false</value>
            <description lang="de">Die Prüfung erfolgt bei jedem Seitenaufruf. Damit kann die Warnung unabhängig vom Prüf-CronJob erfolgen.</description>
        </element>
        <element required="true" type="number">
            <name>monitoredCronsReminderGap</name>
            <label lang="de">Wiederholte Benachrichtigung</label>
            <label lang="en">Maximum reminder gap</label>
            <value>3600</value>
        </element>
        <element type="number">
            <name>monitoredCronsLastRun</name>
            <label lang="de">Letzter Prüflauf</label>
            <value>0</value>
            <options>
                <hideLabel>true</hideLabel>
                <hidden>true</hidden>
            </options>
        </element>
        <element type="number">
            <name>monitoredCronsLastWarning</name>
            <label lang="de">Letzte Warnung</label>
            <value>0</value>
            <options>
                <hideLabel>true</hideLabel>
                <hidden>true</hidden>
            </options>
        </element>
        <element type="number">
            <name>monitoredCronsWarningCount</name>
            <label lang="de">Anzahl Warnungen</label>
            <value>0</value>
            <options>
                <hideLabel>true</hideLabel>
                <hidden>true</hidden>
            </options>
        </element>
        <element type="select">
            <name>preloadFonts_enabled</name>
            <label lang="de">Schriften vorladen aktivieren</label>
            <label lang="en">font preloading enabled</label>
            <value>0</value>
            <store>
                <option>
                    <value>1</value>
                    <label lang="de">Ja</label>
                    <label lang="en">Yes</label>
                </option>
                <option>
                    <value>0</value>
                    <label lang="de">Nein</label>
                    <label lang="en">No</label>
                </option>
            </store>
        </element>
        <element type="combo">
            <name>preloadFonts_standard</name>
            <label lang="en">preload standard fonts</label>
            <label lang="de">Standard Schriften vorladen</label>
            <store>
                <option>
                    <value>shopware</value>
                    <label>Shopware Icons</label>
                </option>
                <option>
                    <value>captcha</value>
                    <label>Shopware Captcha</label>
                </option>
            </store>
            <options>
                <multiSelect>true</multiSelect>
            </options>
        </element>
        <element type="textarea">
            <name>preloadFonts_custom</name>
            <label lang="en">preload customs fonts</label>
            <label lang="de">Eigene Schriftarten vorladen</label>
            <description lang="en">Please enter URL file per line.</description>
            <description lang="de">Bitte eine URL pro Zeile angeben.</description>
        </element>
        <element type="text">
            <name>releasecommit</name>
            <label lang="de">Aktueller Commit</label>
            <label lang="en">Current Commit</label>
            <value></value>
        </element>
    </elements>
</config>
